{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@google/genai": ["https://aistudiocdn.com/@google/genai@1.24.0/dist/index.d.ts"]
    },
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve"
  },
  "include": ["index.tsx"]
}
